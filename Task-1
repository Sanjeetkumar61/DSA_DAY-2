#include <stdio.h>
#include<conio.h>
 

#define MAXSIZE 10

int Queue[MAXSIZE];
int front = 0;
int rear = -1;

void insert();
void delete();
void display();

int main() {
    int choice;

    do {
        printf("\n1. Insert\n2. Delete\n3. Display\n4. Exit\n");
        printf("Enter your choice: ");
        scanf("%d", &choice);

        switch (choice) {
            case 1:
                insert();
                break;
            case 2:
                delete();
                break;
            case 3:
                display();
                break;
            case 4:
                printf("Exiting...\n");
                break;
            default:
                printf("Invalid choice! Please try again.\n");
        }
    } while (choice != 4);

    return 0;
}

void insert() {
    int n;
    if (rear == MAXSIZE - 1) {
        printf("Queue Overflow\n");
    } else {
        printf("Enter an element: ");
        scanf("%d", &n);
        rear++;
        Queue[rear] = n;
        printf("Element inserted: %d\n", n);
    }
}

void delete() {
    if (front > rear) {
        printf("Queue Underflow\n");
    } else {
        int n = Queue[front];
        front++;
        printf("Deleted element is %d\n", n);
    }
}

void display() {
    if (front > rear) {
        printf("Queue is empty\n");
    } else {
        printf("Queue elements are:\n");
        for (int i = front; i <= rear; i++) {
            printf("%d\n", Queue[i]);
        }
    }
}
